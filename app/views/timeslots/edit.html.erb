<h1>Editing timeslot</h1>

<% form_for(@timeslot) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :openings %><br />
    <%= f.text_field :openings %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @timeslot %> |
<%= link_to 'Back', timeslots_path %>